    "/api/applications/apply": {
      "post": {
        "tags": [
          "JobApplications"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ApplyJobDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ApplyJobDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ApplyJobDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/applications/job/{jobId}": {
      "get": {
        "tags": [
          "JobApplications"
        ],
        "parameters": [
          {
            "name": "jobId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/applications/my": {
      "get": {
        "tags": [
          "JobApplications"
        ],
        "responses": {
          "200": {
